@import '_normalize'

@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?p3qvkk');
	src:url('../fonts/icomoon.eot?#iefixp3qvkk') format('embedded-opentype'),
		url('../fonts/icomoon.ttf?p3qvkk') format('truetype'),
		url('../fonts/icomoon.woff?p3qvkk') format('woff'),
		url('../fonts/icomoon.svg?p3qvkk#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-ch:before {
    content: "\f03a";
}
.icon-prev:before {
    content: "\f0a4";
}
.icon-next:before {
    content: "\f078";
}
.icon-lc:before {
    content: "\f060";
}
.icon-sb:before {
    content: "\f03b";
}
.icon-menu:before {
    content: "\f05e";
}
.icon-close:before {
    content: "\f081";
}
.icon-ph:before {
    content: "\e900";
}
.icon-zoom:before {
    content: "\e987";
}


